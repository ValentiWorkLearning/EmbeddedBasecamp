echo "First Argument is: $1"

if [ $1 = "-help"] || [ $1 = "-h" ]
then

	echo "Place a directory filename to specify delete\n"
	echo "Add --help or -h to print help
"
fi

fileList=$(find $1 -name '~*.*' -o -name '*.tmp' -o -name '-*' -o -name '_*.*')

for fileItem in $fileList; do
	echo $fileItem
	rm $fileItem
done
